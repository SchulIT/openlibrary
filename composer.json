{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^3.10.4",
        "doctrine/doctrine-bundle": "^2.18.2",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.1",
        "dragonmantank/cron-expression": "^3.6",
        "easycorp/easyadmin-bundle": "^4.27.6",
        "gedmo/doctrine-extensions": "^3.22",
        "jbtronics/settings-bundle": "^3.1.3",
        "knplabs/knp-menu-bundle": "^3.7",
        "league/csv": "^9.28.0",
        "lightsaml2/lightsaml": ">=3.0.1",
        "lightsaml2/sp-bundle": "^5.0",
        "lightsaml2/symfony-bridge": "^6.0.0",
        "lorisleiva/cron-translator": "^0.4.6",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.1",
        "psr/simple-cache": "^3.0",
        "schulit/common-bundle": "^6.0.2",
        "stof/doctrine-extensions-bundle": ">=1.15.2",
        "symfony/asset": "^7.4",
        "symfony/clock": "^7.4",
        "symfony/console": "^7.4.3",
        "symfony/doctrine-messenger": "^7.4.3",
        "symfony/dotenv": "^7.4",
        "symfony/expression-language": "^7.4",
        "symfony/flex": "^2.10",
        "symfony/form": "^7.4.3",
        "symfony/framework-bundle": "^7.4.3",
        "symfony/http-client": "^7.4.3",
        "symfony/intl": "^7.4",
        "symfony/mailer": "^7.4.3",
        "symfony/mime": "^7.4",
        "symfony/monolog-bundle": "^3.11.1",
        "symfony/process": "^7.4.3",
        "symfony/property-access": "^7.4.3",
        "symfony/property-info": "^7.4.3",
        "symfony/rate-limiter": "^7.4",
        "symfony/runtime": "^7.4.1",
        "symfony/scheduler": ">=7.4",
        "symfony/security-bundle": "^7.4",
        "symfony/serializer": "^7.4.3",
        "symfony/string": "^7.4",
        "symfony/translation": "^7.4.3",
        "symfony/twig-bundle": "^7.4.3",
        "symfony/uid": "^7.4",
        "symfony/validator": "^7.4.3",
        "symfony/web-link": "^7.4",
        "symfony/webpack-encore-bundle": "^2.4",
        "symfony/yaml": "^7.4.1",
        "tecnickcom/tc-lib-barcode": "^2.4.18",
        "tecnickcom/tcpdf": "^6.10.1",
        "twig/extra-bundle": "^2.12|^3.22.2",
        "twig/intl-extra": "^3.22.1",
        "twig/twig": "^2.12|^3.22.2",
        "vich/uploader-bundle": "^2.9.1",
        "zenstruck/messenger-monitor-bundle": "^0.6.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "tbachert/spi": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5.4",
        "symfony/browser-kit": "^7.4.3",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "^7.4",
        "symfony/web-profiler-bundle": "^7.4.3"
    }
}
